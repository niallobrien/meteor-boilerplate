     if (e.name === 'MongoError' || e.name === 'MinimongoError') {                                               // 905\n            throw new Meteor.Error(409, e.toString());                                                                // 906\n          } else {                                                                                                    // 907\n            throw e;                                                                                                  // 908\n          }                                                                                                           // 909\n        }                                                                                                             // 910\n      };                                                                                                              // 911\n    });                                                                                                               // 912\n    // Minimongo on the server gets no stubs; instead, by default                                                     // 913\n    // it wait()s until its result is ready, yielding.                                                                // 914\n    // This matches the behavior of macromongo on the server better.                                                  // 915\n    // XXX see #MeteorServerNull                                                                                      // 916\n    if (Meteor.isClient || self._connection === Meteor.server)                                                        // 917\n      self._connection.methods(m);                                                                                    // 918\n  }                                                                                                                   // 919\n};                                                                                                                    // 920\n                                                                                                                      // 921\n                                                                                                                      // 922\nMongo.Collection.prototype._updateFetch = function (fields) {                                                         // 923\n  var self = this;                                                                                                    // 924\n                                                                                                                      // 925\n  if (!self._validators.fetchAllFields) {                                                                             // 926\n    if (fields) {                                                                                                     // 927\n      self._validators.fetch = _.union(self._validators.fetch, fields);                                               // 928\n    } else {                                                                                                          // 929\n      self._validators.fetchAllFields = true;                                                                         // 930\n      // clear fetch just to make sure we don't accidentally read it                                                  // 931\n      self._validators.fetch = null;                                                                                  // 932\n    }                                                                                                                 // 933\n  }                                                                                                                   // 934\n};                                                                                                                    // 935\n                                                                                                                      // 936\nMongo.Collection.prototype._isInsecure = function () {                                                                // 937\n  var self = this;                                                                                                    // 938\n  if (self._insecure === undefined)                                                                                   // 939\n    return !!Package.insecure;                                                                                        // 940\n  return self._insecure;                                                                                              // 941\n};                                                                                                                    // 942\n                                                                                                                      // 943\nvar docToValidate = function (validator, doc, generatedId) {                                                          // 944\n  var ret = doc;                                                                                                      // 945\n  if (validator.transform) {                                                                                          // 946\n    ret = EJSON.clone(doc);                                                                                           // 947\n    // If you set a server-side transform on your collection, then you don't get                                      // 948\n    // to tell the difference between \"client specified the ID\" and \"server                                           // 949\n    // generated the ID\", because transforms expect to get _id.  If you want to                                       // 950\n    // do that check, you can do it with a specific                                                                   // 951\n    // `C.allow({insert: f, transform: null})` validator.                                                             // 952\n    if (generatedId !== null) {                                                                                       // 953\n      ret._id = generatedId;                                                                                          // 954\n    }                                                                                                                 // 955\n    ret = validator.transform(ret);                                                                                   // 956\n  }                                                                                                                   // 957\n  return ret;                                                                                                         // 958\n};                                                                                                                    // 959\n                                                                                                                      // 960\nMongo.Collection.prototype._validatedInsert = function (userId, doc,                                                  // 961\n                                                         generatedId) {                                               // 962\n  var self = this;                                                                                                    // 963\n                                                                                                                      // 964\n  // call user validators.                                                                                            // 965\n  // Any deny returns true means denied.                                                                              // 966\n  if (_.any(self._validators.insert.deny, function(validator) {                                                       // 967\n    return validator(userId, docToValidate(validator, doc, generatedId));                                             // 968\n  })) {                                                                                                               // 969\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 970\n  }                                                                                                                   // 971\n  // Any allow returns true means proceed. Throw error if they all fail.                                              // 972\n  if (_.all(self._validators.insert.allow, function(validator) {                                                      // 973\n    return !validator(userId, docToValidate(validator, doc, generatedId));                                            // 974\n  })) {                                                                                                               // 975\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 976\n  }                                                                                                                   // 977\n                                                                                                                      // 978\n  // If we generated an ID above, insert it now: after the validation, but                                            // 979\n  // before actually inserting.                                                                                       // 980\n  if (generatedId !== null)                                                                                           // 981\n    doc._id = generatedId;                                                                                            // 982\n                                                                                                                      // 983\n  self._collection.insert.call(self._collection, doc);                                                                // 984\n};                                                                                                                    // 985\n                                                                                                                      // 986\nvar transformDoc = function (validator, doc) {                                                                        // 987\n  if (validator.transform)                                                                                            // 988\n    return validator.transform(doc);                                                                                  // 989\n  return doc;                                                                                                         // 990\n};                                                                                                                    // 991\n                                                                                                                      // 992\n// Simulate a mongo `update` operation while validating that the access                                               // 993\n// control rules set by calls to `allow/deny` are satisfied. If all                                                   // 994\n// pass, rewrite the mongo operation to use $in to set the list of                                                    // 995\n// document ids to change ##ValidatedChange                                                                           // 996\nMongo.Collection.prototype._validatedUpdate = function(                                                               // 997\n    userId, selector, mutator, options) {                                                                             // 998\n  var self = this;                                                                                                    // 999\n                                                                                                                      // 1000\n  check(mutator, Object);                                                                                             // 1001\n                                                                                                                      // 1002\n  options = _.clone(options) || {};                                                                                   // 1003\n                                                                                                                      // 1004\n  if (!LocalCollection._selectorIsIdPerhapsAsObject(selector))                                                        // 1005\n    throw new Error(\"validated update should be of a single ID\");                                                     // 1006\n                                                                                                                      // 1007\n  // We don't support upserts because they don't fit nicely into allow/deny                                           // 1008\n  // rules.                                                                                                           // 1009\n  if (options.upsert)                                                                                                 // 1010\n    throw new Meteor.Error(403, \"Access denied. Upserts not \" +                                                       // 1011\n                           \"allowed in a restricted collection.\");                                                    // 1012\n                                                                                                                      // 1013\n  var noReplaceError = \"Access denied. In a restricted collection you can only\" +                                     // 1014\n        \" update documents, not replace them. Use a Mongo update operator, such \" +                                   // 1015\n        \"as '$set'.\";                                                                                                 // 1016\n                                                                                                                      // 1017\n  // compute modified fields                                                                                          // 1018\n  var fields = [];                                                                                                    // 1019\n  if (_.isEmpty(mutator)) {                                                                                           // 1020\n    throw new Meteor.Error(403, noReplaceError);                                                                      // 1021\n  }                                                                                                                   // 1022\n  _.each(mutator, function (params, op) {                                                                             // 1023\n    if (op.charAt(0) !== '$') {                                                                                       // 1024\n      throw new Meteor.Error(403, noReplaceError);                                                                    // 1025\n    } else if (!_.has(ALLOWED_UPDATE_OPERATIONS, op)) {                                                               // 1026\n      throw new Meteor.Error(                                                                                         // 1027\n        403, \"Access denied. Operator \" + op + \" not allowed in a restricted collection.\");                           // 1028\n    } else {                                                                                                          // 1029\n      _.each(_.keys(params), function (field) {                                                                       // 1030\n        // treat dotted fields as if they are replacing their                                                         // 1031\n        // top-level part                                                                                             // 1032\n        if (field.indexOf('.') !== -1)                                                                                // 1033\n          field = field.substring(0, field.indexOf('.'));                                                             // 1034\n                                                                                                                      // 1035\n        // record the field we are trying to change                                                                   // 1036\n        if (!_.contains(fields, field))                                                                               // 1037\n          fields.push(field);                                                                                         // 1038\n      });                                                                                                             // 1039\n    }                                                                                                                 // 1040\n  });                                                                                                                 // 1041\n                                                                                                                      // 1042\n  var findOptions = {transform: null};                                                                                // 1043\n  if (!self._validators.fetchAllFields) {                                                                             // 1044\n    findOptions.fields = {};                                                                                          // 1045\n    _.each(self._validators.fetch, function(fieldName) {                                                              // 1046\n      findOptions.fields[fieldName] = 1;                                                                              // 1047\n    });                                                                                                               // 1048\n  }                                                                                                                   // 1049\n                                                                                                                      // 1050\n  var doc = self._collection.findOne(selector, findOptions);                                                          // 1051\n  if (!doc)  // none satisfied!                                                                                       // 1052\n    return 0;                                                                                                         // 1053\n                                                                                                                      // 1054\n  // call user validators.                                                                                            // 1055\n  // Any deny returns true means denied.                                                                              // 1056\n  if (_.any(self._validators.update.deny, function(validator) {                                                       // 1057\n    var factoriedDoc = transformDoc(validator, doc);                                                                  // 1058\n    return validator(userId,                                                                                          // 1059\n                     factoriedDoc,                                                                                    // 1060\n                     fields,                                                                                          // 1061\n                     mutator);                                                                                        // 1062\n  })) {                                                                                                               // 1063\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 1064\n  }                                                                                                                   // 1065\n  // Any allow returns true means proceed. Throw error if they all fail.                                              // 1066\n  if (_.all(self._validators.update.allow, function(validator) {                                                      // 1067\n    var factoriedDoc = transformDoc(validator, doc);                                                                  // 1068\n    return !validator(userId,                                                                                         // 1069\n                      factoriedDoc,                                                                                   // 1070\n                      fields,                                                                                         // 1071\n                      mutator);                                                                                       // 1072\n  })) {                                                                                                               // 1073\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 1074\n  }                                                                                                                   // 1075\n                                                                                                                      // 1076\n  options._forbidReplace = true;                                                                                      // 1077\n                                                                                                                      // 1078\n  // Back when we supported arbitrary client-provided selectors, we actually                                          // 1079\n  // rewrote the selector to include an _id clause before passing to Mongo to                                         // 1080\n  // avoid races, but since selector is guaranteed to already just be an ID, we                                       // 1081\n  // don't have to any more.                                                                                          // 1082\n                                                                                                                      // 1083\n  return self._collection.update.call(                                                                                // 1084\n    self._collection, selector, mutator, options);                                                                    // 1085\n};                                                                                                                    // 1086\n                                                                                                                      // 1087\n// Only allow these operations in validated updates. Specifically                                                     // 1088\n// whitelist operations, rather than blacklist, so new complex                                                        // 1089\n// operations that are added aren't automatically allowed. A complex                                                  // 1090\n// operation is one that does more than just modify its target                                                        // 1091\n// field. For now this contains all update operations except '$rename'.                                               // 1092\n// http://docs.mongodb.org/manual/reference/operators/#update                                                         // 1093\nvar ALLOWED_UPDATE_OPERATIONS = {                                                                                     // 1094\n  $inc:1, $set:1, $unset:1, $addToSet:1, $pop:1, $pullAll:1, $pull:1,                                                 // 1095\n  $pushAll:1, $push:1, $bit:1                                                                                         // 1096\n};                                                                                                                    // 1097\n                                                                                                                      // 1098\n// Simulate a mongo `remove` operation while validating access control                                                // 1099\n// rules. See #ValidatedChange                                                                                        // 1100\nMongo.Collection.prototype._validatedRemove = function(userId, selector) {                                            // 1101\n  var self = this;                                                                                                    // 1102\n                                                                                                                      // 1103\n  var findOptions = {transform: null};                                                                                // 1104\n  if (!self._validators.fetchAllFields) {                                                                             // 1105\n    findOptions.fields = {};                                                                                          // 1106\n    _.each(self._validators.fetch, function(fieldName) {                                                              // 1107\n      findOptions.fields[fieldName] = 1;                                                                              // 1108\n    });                                                                                                               // 1109\n  }                                                                                                                   // 1110\n                                                                                                                      // 1111\n  var doc = self._collection.findOne(selector, findOptions);                                                          // 1112\n  if (!doc)                                                                                                           // 1113\n    return 0;                                                                                                         // 1114\n                                                                                                                      // 1115\n  // call user validators.                                                                                            // 1116\n  // Any deny returns true means denied.                                                                              // 1117\n  if (_.any(self._validators.remove.deny, function(validator) {                                                       // 1118\n    return validator(userId, transformDoc(validator, doc));                                                           // 1119\n  })) {                                                                                                               // 1120\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 1121\n  }                                                                                                                   // 1122\n  // Any allow returns true means proceed. Throw error if they all fail.                                              // 1123\n  if (_.all(self._validators.remove.allow, function(validator) {                                                      // 1124\n    return !validator(userId, transformDoc(validator, doc));                                                          // 1125\n  })) {                                                                                                               // 1126\n    throw new Meteor.Error(403, \"Access denied\");                                                                     // 1127\n  }                                                                                                                   // 1128\n                                                                                                                      // 1129\n  // Back when we supported arbitrary client-provided selectors, we actually                                          // 1130\n  // rewrote the selector to {_id: {$in: [ids that we found]}} before passing to                                      // 1131\n  // Mongo to avoid races, but since selector is guaranteed to already just be                                        // 1132\n  // an ID, we don't have to any more.                                                                                // 1133\n                                                                                                                      // 1134\n  return self._collection.remove.call(self._collection, selector);                                                    // 1135\n};                                                                                                                    // 1136\n                                                                                                                      // 1137\n/**                                                                                                                   // 1138\n * @deprecated in 0.9.1                                                                                               // 1139\n */                                                                                                                   // 1140\nMeteor.Collection = Mongo.Collection;                                                                                 // 1141\n                                                                                                                      // 1142\n////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n\n\n/* Exports */\nif (typeof Package === 'undefined') Package = {};\nPackage.mongo = {\n  Mongo: Mongo\n};\n\n})();\n","servePath":"/packages/mongo.js","sourceMap":{"version":3,"sources":["/packages/mongo/local_collection_driver.js","/packages/mongo/collection.js"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,2H;;;;;;;;;;;;;;;;;;AC7BA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAC