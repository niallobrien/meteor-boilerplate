   // 809\n  // their collections.                                                                                               // 810\n  self._insecure = undefined;                                                                                         // 811\n                                                                                                                      // 812\n  self._validators = {                                                                                                // 813\n    insert: {allow: [], deny: []},                                                                                    // 814\n    update: {allow: [], deny: []},                                                                                    // 815\n    remove: {allow: [], deny: []},                                                                                    // 816\n    upsert: {allow: [], deny: []}, // dummy arrays; can't set these!                                                  // 817\n    fetch: [],                                                                                                        // 818\n    fetchAllFields: false                                                                                             // 819\n  };                                                                                                                  // 820\n                                                                                                                      // 821\n  if (!self._name)                                                                                                    // 822\n    return; // anonymous collection                                                                                   // 823\n                                                                                                                      // 824\n  // XXX Think about method namespacing. Maybe methods should be                                                      // 825\n  // \"Meteor:Mongo:insert/NAME\"?                                                                                      // 826\n  self._prefix = '/' + self._name + '/';                                                                              // 827\n                                                                                                                      // 828\n  // mutation methods                                                                                                 // 829\n  if (self._connection) {                                                                                             // 830\n    var m = {};                                                                                                       // 831\n                                                                                                                      // 832\n    _.each(['insert', 'update', 'remove'], function (method) {                                                        // 833\n      m[self._prefix + method] = function (/* ... */) {                                                               // 834\n        // All the methods do their own validation, instead of using check().                                         // 835\n        check(arguments, [Match.Any]);                                                                                // 836\n        var args = _.toArray(arguments);                                                                              // 837\n        try {                                                                                                         // 838\n          // For an insert, if the client didn't specify an _id, generate one                                         // 839\n          // now; because this uses DDP.randomStream, it will be consistent with                                      // 840\n          // what the client generated. We generate it now rather than later so                                       // 841\n          // that if (eg) an allow/deny rule does an insert to the same                                               // 842\n          // collection (not that it really should), the generated _id will                                           // 843\n          // still be the first use of the stream and will be consistent.                                             // 844\n          //                                                                                                          // 845\n          // However, we don't actually stick the _id onto the document yet,                                          // 846\n          // because we want allow/deny rules to be able to differentiate                                             // 847\n          // between arbitrary client-specified _id fields and merely                                                 // 848\n          // client-controlled-via-randomSeed fields.                                                                 // 849\n          var generatedId = null;                                                                                     // 850\n          if (method === \"insert\" && !_.has(args[0], '_id')) {                                                        // 851\n            generatedId = self._makeNewID();                                                                          // 852\n          }                                                                                                           // 853\n                                                                                                                      // 854\n          if (this.isSimulation) {                                                                                    // 855\n            // In a client simulation, you can do any mutation (even with a                                           // 856\n            // complex selector).                                                                                     // 857\n            if (generatedId !== null)                                                                                 // 858\n              args[0]._id = generatedId;                                                                              // 859\n            return self._collection[method].apply(                                                                    // 860\n              self._collection, args);                                                                                // 861\n          }                                                                                                           // 862\n                                                                                                                      // 863\n          // This is the server receiving a method call from the client.                                              // 864\n                                                                                                                      // 865\n          // We don't allow arbitrary selectors in mutations from the client: only                                    // 866\n          // single-ID selectors.                                                                                     // 867\n          if (method !== 'insert')                                                                                    // 868\n            throwIfSelectorIsNotId(args[0], method);                                                                  // 869\n                                                                                                                      // 870\n          if (self._restricted) {                                                                                     // 871\n            // short circuit if there is no way it will pass.                                                         // 872\n            if (self._validators[method].allow.length === 0) {                                                        // 873\n              throw new Meteor.Error(                                                                                 // 874\n                403, \"Access denied. No allow validators set on restricted \" +                                        // 875\n                  \"collection for method '\" + method + \"'.\");                                                         // 876\n            }                                                                                                         // 877\n                                                                                                                      // 878\n            var validatedMethodName =                                                                                 // 879\n                  '_validated' + method.charAt(0).toUpperCase() + method.slice(1);                                    // 880\n            args.unshift(this.userId);                                                                                // 881\n            method === 'insert' && args.push(generatedId);                                                            // 882\n            return self[validatedMethodName].apply(self, args);                                                       // 883\n          } else if (self._isInsecure()) {                                                                            // 884\n            if (generatedId !== null)                                                                                 // 885\n              args[0]._id = generatedId;                                                                              // 886\n            // In insecure mode, allow any mutation (with a simple selector).                                         // 887\n            // XXX This is kind of bogus.  Instead of blindly passing whatever                                        // 888\n            //     we get