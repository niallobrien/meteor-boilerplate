                                                                                                              // 262\n  CollectionHooks.wrapCollection(Meteor, Meteor);                                                                      // 263\n}                                                                                                                      // 264\n                                                                                                                       // 265\nif (Meteor.isServer) {                                                                                                 // 266\n  var _publish = Meteor.publish;                                                                                       // 267\n  Meteor.publish = function (name, func) {                                                                             // 268\n    return _publish.call(this, name, function () {                                                                     // 269\n      // This function is called repeatedly in publications                                                            // 270\n      var ctx = this, args = arguments;                                                                                // 271\n      return publishUserId.withValue(ctx && ctx.userId, function () {                                                  // 272\n        return func.apply(ctx, args);                                                                                  // 273\n      });                                                                                                              // 274\n    });                                                                                                                // 275\n  };                                                                                                                   // 276\n                                                                                                        